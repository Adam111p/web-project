{
  "name": "layout-auth",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "layout/auth/src",
  "prefix": "app",
  "tags": [],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "layout/auth/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "layout/auth/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "layout/auth/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "layout/auth/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["layout/auth/**/*.ts", "layout/auth/**/*.html"]
      }
    }
  }
}
